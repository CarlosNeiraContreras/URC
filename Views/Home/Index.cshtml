            <img src="/images/assignment 1, utah sunset.jpg" class="slide" width="500" height="300" />
        </div>
        <div class="buttons">
            <a href="#slide-1"></a>
            <a href="#slide-2"></a>
            <a href="#slide-3"></a>
        </div>
    </div>
</body>
