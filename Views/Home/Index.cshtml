    <div class="slider">
        <span id="slide-1"></span>
        <span id="slide-2"></span>
        <span id="slide-3"></span>
        <div class="image-container">
            <img src="~/images/assignment 1 school emblem.jpg" class="slide" width="500" height="300" />
